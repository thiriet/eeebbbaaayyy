{
    "name": "gremo/buzz-bundle",
    "type": "symfony-bundle",
    "description": "Symfony 2 Bundle for using the lightweight Buzz HTTP client.",
    "keywords": ["buzz", "http client"],
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Gremo",
            "homepage": "https://github.com/gremo"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/dependency-injection": ">=2.0.0,<2.3-dev",
        "symfony/http-kernel": ">=2.0.0,<2.3-dev",
        "symfony/config": ">=2.0.0,<2.3-dev",
        "kriswallsmith/buzz": ">= 0.6,<1.0"
    },
    "autoload": {
        "psr-0": {
            "Gremo\\BuzzBundle": ""
        }
    },
    "target-dir" : "Gremo/BuzzBundle"
}
